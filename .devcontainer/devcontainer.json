{
  "name": "Python Poetry DevContainer",
  "dockerComposeFile": [
    "../compose.yml"
  ],
  "service": "app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.flake8"
      ],
      "settings": {
        "python.formatting.provider": "black",
        "python.linting.flake8Enabled": true
      }
    }
  },
  "workspaceFolder": "/app/src"
}